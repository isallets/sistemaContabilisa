{
  "version": 2,
  "builds": [
    { "src": "api/server.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/server.ts" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/index", "dest": "/index.html" }
  ]
}
